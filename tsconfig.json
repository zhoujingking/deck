{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "moduleResolution": "NodeNext",
    "target": "ES6",
    "outDir": "lib",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "allowJs": true
  },
  "include": ["src/**/*"]
}